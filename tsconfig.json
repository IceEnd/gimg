{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./",
    "sourceMap": false,
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "**.ts",
    "**.json"
  ],
  "exclude": [
    "node_modules",
  ]
}
